    "moment": "^2.29.4",
    "mongoose": "^7.3.4",
    "nekos.life": "^3.0.0",
    "node-fetch": "^2.6.12",
    "openai": "^4.11.1",
    "os": "^0.1.2",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.1",
    "pretty-ms": "^8.0.0",
    "snakecord": "^1.0.9",
    "sourcebin_js": "^0.0.3-ignore",
