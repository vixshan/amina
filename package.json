    "eslint": "^8.45.0",
    "eslint-plugin-jsdoc": "^46.4.3",
    "node": "^21.1.0",
    "nodemon": "^3.0.1",
    "prettier": "3.0.3"
  },
  "keywords": [
    "discord",
    "discord.js",
    "discord-bot",
    "open-source",
    "bot",
    "multipurpose",
    "multi-purpose-bot"
